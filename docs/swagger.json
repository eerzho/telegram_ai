{
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "title": "TelegramAI API",
        "contact": {}
    },
    "host": "localhost",
    "basePath": "/",
    "paths": {
        "/_hc": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "summary": "health check",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/healthcheck.Output"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/json.Error"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/json.Error"
                        }
                    }
                }
            }
        },
        "/v1/improvements/generate": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json",
                    "text/event-stream"
                ],
                "tags": [
                    "improvement"
                ],
                "summary": "improvement generate",
                "parameters": [
                    {
                        "description": "body",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/improvementgenerate.Input"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/sse.Event"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/json.Error"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/json.Error"
                        }
                    }
                }
            }
        },
        "/v1/responses/generate": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json",
                    "text/event-stream"
                ],
                "tags": [
                    "response"
                ],
                "summary": "response generate",
                "parameters": [
                    {
                        "description": "body",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/responsegenerate.Input"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/sse.Event"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/json.Error"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/json.Error"
                        }
                    }
                }
            }
        },
        "/v1/summaries/generate": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json",
                    "text/event-stream"
                ],
                "tags": [
                    "summary"
                ],
                "summary": "summary generate",
                "parameters": [
                    {
                        "description": "body",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/summarygenerate.Input"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/sse.Event"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/json.Error"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/json.Error"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "healthcheck.Output": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            }
        },
        "improvementgenerate.Input": {
            "type": "object",
            "required": [
                "text"
            ],
            "properties": {
                "text": {
                    "type": "string",
                    "minLength": 1
                }
            }
        },
        "json.Error": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "details": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/json.ErrorDetail"
                    }
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "json.ErrorDetail": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "field": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "responsegenerate.Input": {
            "type": "object",
            "required": [
                "messages",
                "owner"
            ],
            "properties": {
                "messages": {
                    "type": "array",
                    "maxItems": 1000,
                    "minItems": 1,
                    "items": {
                        "$ref": "#/definitions/responsegenerate.InputMessage"
                    }
                },
                "owner": {
                    "$ref": "#/definitions/responsegenerate.InputUser"
                }
            }
        },
        "responsegenerate.InputMessage": {
            "type": "object",
            "required": [
                "date",
                "sender",
                "text"
            ],
            "properties": {
                "date": {
                    "type": "integer"
                },
                "sender": {
                    "$ref": "#/definitions/responsegenerate.InputUser"
                },
                "text": {
                    "type": "string",
                    "minLength": 1
                }
            }
        },
        "responsegenerate.InputUser": {
            "type": "object",
            "required": [
                "chat_id",
                "name"
            ],
            "properties": {
                "chat_id": {
                    "type": "string",
                    "minLength": 1
                },
                "name": {
                    "type": "string",
                    "minLength": 1
                }
            }
        },
        "sse.Event": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "retry": {
                    "description": "milliseconds",
                    "type": "integer"
                }
            }
        },
        "summarygenerate.Input": {
            "type": "object",
            "required": [
                "language",
                "messages",
                "owner",
                "peer"
            ],
            "properties": {
                "language": {
                    "type": "string",
                    "maxLength": 2,
                    "minLength": 2
                },
                "messages": {
                    "type": "array",
                    "maxItems": 1000,
                    "minItems": 1,
                    "items": {
                        "$ref": "#/definitions/summarygenerate.InputMessage"
                    }
                },
                "owner": {
                    "$ref": "#/definitions/summarygenerate.InputSender"
                },
                "peer": {
                    "$ref": "#/definitions/summarygenerate.InputSender"
                }
            }
        },
        "summarygenerate.InputMessage": {
            "type": "object",
            "required": [
                "date",
                "sender",
                "text"
            ],
            "properties": {
                "date": {
                    "type": "integer"
                },
                "sender": {
                    "$ref": "#/definitions/summarygenerate.InputSender"
                },
                "text": {
                    "type": "string",
                    "minLength": 1
                }
            }
        },
        "summarygenerate.InputSender": {
            "type": "object",
            "required": [
                "chat_id",
                "name"
            ],
            "properties": {
                "chat_id": {
                    "type": "string",
                    "minLength": 1
                },
                "name": {
                    "type": "string",
                    "minLength": 1
                }
            }
        }
    },
    "externalDocs": {
        "description": "GitHub",
        "url": "https://github.com/eerzho/telegram-ai"
    }
}